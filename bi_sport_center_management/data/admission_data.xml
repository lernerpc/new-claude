<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <record id="seq_student_admission" model="ir.sequence">
            <field name="name">Student Admission Sequence</field>
            <field name="code">student.admission</field>
            <field name="prefix">LC/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_student_inquiry" model="ir.sequence">
            <field name="name">Student Inquiry Sequence</field>
            <field name="code">student.inquiry</field>
            <field name="prefix">INQ/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_center_booking" model="ir.sequence">
            <field name="name">Center Booking Sequence</field>
            <field name="code">center.booking</field>
            <field name="prefix">%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_center_certificate" model="ir.sequence">
            <field name="name">Certificate Sequence</field>
            <field name="code">center.certificate</field>
            <field name="prefix">%(year)s%(month)s%(day)s</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>